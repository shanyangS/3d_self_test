    .scope S_0x56463dc7efd0;
T_1 ;
    %wait E_0x56463dc80010;
    %load/vec4 v0x56463dc93ef0_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x56463dc93d20_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x56463dc93e30_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x56463dc93e30_0, 0;
T_1.1 ;
    %load/vec4 v0x56463dc93d20_0;
    %assign/vec4 v0x56463dc93ef0_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56463dc7efd0;
T_2 ;
    %wait E_0x56463dc80010;
    %load/vec4 v0x56463dc45350_0;
    %addi 1, 0, 9;
    %assign/vec4 v0x56463dc45350_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x56463dc7efd0;
T_3 ;
    %wait E_0x56463dc80010;
    %load/vec4 v0x56463dc93e30_0;
    %load/vec4 v0x56463dc93c60_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x56463dc45350_0;
    %assign/vec4 v0x56463dc45ba0_0, 0;
T_3.0 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x56463dc7efd0;
T_4 ;
    %wait E_0x56463dc80010;
    %load/vec4 v0x56463dc45350_0;
    %parti/s 1, 8, 5;
    %store/vec4 v0x56463dc93c60_0, 0, 1;
    %jmp T_4;
